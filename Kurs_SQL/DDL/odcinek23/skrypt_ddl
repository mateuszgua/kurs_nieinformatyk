

/*
DROP USER SKLEP CASCADE;

ALTER SESSION SET CONTAINER=XEPDB1;

CREATE USER SKLEP IDENTIFIED BY 123;

GRANT CREATE SESSION TO SKLEP;

GRANT CREATE TABLE TO SKLEP;

GRANT CREATE ANY DIRECTORY TO SKLEP;

GRANT CREATE TRIGGER TO SKLEP;

GRANT UNLIMITED TABLESPACE TO SKLEP;

DROP DIRECTORY EXTERNAL_TABLES_DIR;

*/


 CREATE TABLE SKLEP.PRACOWNICY 
   (
      ID 		 		NUMBER 
	, IMIE 	 		    VARCHAR2(30) NOT NULL
	, NAZWISKO 		    VARCHAR2(60) NOT NULL
	, PESEL 	 		VARCHAR2(11) NOT NULL 
	, PLEC 			    VARCHAR2(1)  NOT NULL 
	, DATA_URODZENIA    DATE
	, PENSJA 			NUMBER 
	, KOD_DEPARTAMENTU  NUMBER
    , CHECK (plec in ('M', 'K')) 
    , CONSTRAINT PRACOWNICY_PK PRIMARY KEY (ID)
	--  FOREIGN KEY (KOD_DEPARTAMENTU)REFERENCES SKLEP.DEPARTAMENTY (KOD_DEPARTAMENTU)
	);

  CREATE TABLE SKLEP.DEPARTAMENTY 
   (
     KOD_DEPARTAMENTU 	NUMBER
   , NAZWA_DEPARTAMENTU VARCHAR2(40) 
   , ID_KIEROWNIKA 		NUMBER 			NOT NULL 
   , MIEJSCOWOSC 		VARCHAR2(50)
   , CONSTRAINT PK_DEPARTAMENTY PRIMARY KEY (KOD_DEPARTAMENTU)
   , CONSTRAINT FK_DEP_PRACOWNIK FOREIGN KEY (ID_KIEROWNIKA)
	 REFERENCES SKLEP.PRACOWNICY (ID)
   );
   
   
	 CREATE TABLE SKLEP.TYPY_PRODUKTOW 
	(
	  TYP_PRODUKTU   VARCHAR2(20)
	, NAZWA_PELNA    VARCHAR2(40) NOT NULL
	, RABAT 		 NUMBER 
	, OKRES_WAZNOSCI VARCHAR2(1)
	, CHECK (okres_waznosci in ('K', 'D'))
	, PRIMARY KEY (TYP_PRODUKTU)
	);
	
 
  CREATE TABLE SKLEP.PRODUKTY 
   (
      ID 				NUMBER
	, NAZWA_PRODUKTU    VARCHAR2(100)
	, TYP_PRODUKTU 		VARCHAR2(30) 
	, CENA_SPRZEDAZY 	NUMBER 
	, CENA_ZAKUPU 		NUMBER
	, STAWKA_PODATKU 	NUMBER
	, JM 				VARCHAR2(10) 
	, CONSTRAINT PK_PRODUKTY PRIMARY KEY (ID)
	, CONSTRAINT FK_TYP_PRODUKTU FOREIGN KEY (TYP_PRODUKTU)
	  REFERENCES SKLEP.TYPY_PRODUKTOW (TYP_PRODUKTU) ENABLE
   )
   ;

  CREATE TABLE SKLEP.RODZAJE_SRODKOW_TRANSPORTU 
   (  KOD 			VARCHAR2(10) 
	, PELNA_NAZWA 	VARCHAR2(80) NOT NULL ENABLE
	, MIN_DLUGOSC 	NUMBER
	, MAX_DLUGOSC 	NUMBER
	, MIN_WAGA 		NUMBER
	, MAX_WAGA 		NUMBER
	, LADOWNOSC 	NUMBER
	, PRIMARY KEY (KOD)
  );

  CREATE TABLE SKLEP.DOSTAWCY 
   (
      ID_DOSTAWCY 					    NUMBER
	, NAZWA_DOSTAWCY 					VARCHAR2(50) NOT NULL 
	, KOD_RODZAJU_SRODKA_TRANSPORTU 	VARCHAR2(10)
	, STATUS_AKTYWNOSCI 				VARCHAR2(1)
	, PRIMARY KEY (ID_DOSTAWCY)
	, FOREIGN KEY (KOD_RODZAJU_SRODKA_TRANSPORTU)
	  REFERENCES SKLEP.RODZAJE_SRODKOW_TRANSPORTU (KOD)
   );


  CREATE TABLE SKLEP.PRZYCZYNY_ABSENCJI 
   (
      ID 		NUMBER 
	, NAZWA 	VARCHAR2(200) 	NOT NULL
	, CZY_ZGODA VARCHAR2(1) 	DEFAULT 'N'
	, UWAGI 	VARCHAR2(1000)
	, PRIMARY KEY (ID)
  );

  CREATE TABLE SKLEP.ABSENCJE 
   (
      ID 			  NUMBER
	, ID_PRACOWNIKA   NUMBER
	, PA_KOD 		  NUMBER
	, DATA_OD 	  	  DATE
	, DATA_DO         DATE
	, PRIMARY KEY (ID)
	, FOREIGN KEY (ID_PRACOWNIKA) REFERENCES SKLEP.PRACOWNICY (ID)
    , FOREIGN KEY (PA_KOD) REFERENCES SKLEP.PRZYCZYNY_ABSENCJI (ID)
   );

  CREATE TABLE SKLEP.KLIENCI 
   (  ID 		     NUMBER
	, IMIE 	         VARCHAR2(30) NOT NULL
	, NAZWISKO  	 VARCHAR2(60) NOT NULL
	, PESEL 		 VARCHAR2(11) 
	, KOD_POCZTOWY   VARCHAR2(10)
	, MIEJSCOWOSC    VARCHAR2(40)
	, PRIMARY KEY (ID)
  );

  CREATE TABLE SKLEP.FAKTURY_SPRZEDAZY 
   (
      ID_FAKTURY 		NUMBER
	, DATA_FAKTURY    	DATE 
	, ID_KLIENTA 		NUMBER 
	, WARTOSC_NETTO 	NUMBER 
	, WARTOSC_BRUTTO 	NUMBER 
	, STAWKA_PODATKU 	NUMBER 
	, WALUTA 			VARCHAR2(4) 
	, PRIMARY KEY (ID_FAKTURY)
	, FOREIGN KEY (ID_KLIENTA)REFERENCES SKLEP.KLIENCI (ID)
   );

  CREATE TABLE SKLEP.ZAKUPY 
   (
      ID_ZAKUPU 	NUMBER
	, ID_KLIENTA 	NUMBER 
	, CZY_PARAGON VARCHAR2(1) 
	, CZY_FAKTURA VARCHAR2(1)
	, ID_FAKTURY 	NUMBER
	, DATA_ZAKUPU DATE
	, PRIMARY KEY (ID_ZAKUPU)
	, FOREIGN KEY (ID_KLIENTA)REFERENCES SKLEP.KLIENCI (ID)
	, FOREIGN KEY (ID_FAKTURY) REFERENCES SKLEP.FAKTURY_SPRZEDAZY (ID_FAKTURY)
   ) ;

  CREATE TABLE SKLEP.ZAKUPY_POZYCJE 
   (
      ID_ZAKUPU 		NUMBER
	, NUMER_POZYCJI 	NUMBER 
	, ID_PRODUKTU 		NUMBER 
	, ILOSC 			NUMBER 
	, CZY_REKLAMACJA  	VARCHAR2(1) 
	, PRIMARY KEY (ID_ZAKUPU, NUMER_POZYCJI)
	, FOREIGN KEY (ID_ZAKUPU) REFERENCES SKLEP.ZAKUPY (ID_ZAKUPU)
	, FOREIGN KEY (ID_PRODUKTU)REFERENCES SKLEP.PRODUKTY (ID)
   );

  CREATE TABLE SKLEP.JEDNOSTKI_PRODUKTOW 
   (
      ID NUMBER
	, ID_PRODUKTU NUMBER
	, JM VARCHAR2(10) 
	, JM_ALT VARCHAR2(10) 
	, PRZELICZNIK NUMBER
	, PRIMARY KEY (ID)
	, FOREIGN KEY (ID_PRODUKTU)REFERENCES SKLEP.PRODUKTY (ID)
   )
   ;

	CREATE DIRECTORY EXTERNAL_TABLES_DIR AS 'C:\app\Darek\product\18.0.0\external_tables';

	
  CREATE TABLE SKLEP.RODZAJE_PRODUKTOW 
   (
      KOD 				VARCHAR2(10)
	, NAZWA 			VARCHAR2(50)
	, WYMAGANA_KONCESJA CHAR(1)
   ) 
   ORGANIZATION EXTERNAL 
    ( TYPE ORACLE_LOADER
      DEFAULT DIRECTORY EXTERNAL_TABLES_DIR
      ACCESS PARAMETERS
      ( FIELDS TERMINATED BY ';'
       )
      LOCATION
       ( 'product_type.csv'
       )
    );

  CREATE TABLE SKLEP.PRACOWNICY_ARCHIWUM 
   (
      EVENT_DATE 			 DATE 
	, EVENT_TYPE 			 VARCHAR2(20)
	, USER_NAME 			 VARCHAR2(20)
	, OLD_ID 				 NUMBER
	, OLD_IMIE 			 	 VARCHAR2(30)
	, OLD_NAZWISKO 		  	 VARCHAR2(60)
	, OLD_PESEL 			 VARCHAR2(11)
	, OLD_PLEC 			 	 VARCHAR2(1)
	, OLD_DATA_URODZENIA   	 DATE
	, OLD_PENSJA 			 NUMBER
	, OLD_KOD_DEPARTAMENTU 	 NUMBER
	, NEW_ID 				 NUMBER
	, NEW_IMIE 			   	 VARCHAR2(30)
	, NEW_NAZWISKO 		   	 VARCHAR2(60) 
	, NEW_PESEL 			 VARCHAR2(11)
	, NEW_PLEC 			 	 VARCHAR2(1)
	, NEW_DATA_URODZENIA   	 DATE
	, NEW_PENSJA 			 NUMBER
	, NEW_KOD_DEPARTAMENTU 	 NUMBER
   );
  
  
	CREATE OR REPLACE TRIGGER AIUD_ARCHIWIZUJ
	AFTER INSERT OR UPDATE OR DELETE
	ON PRACOWNICY
	FOR EACH ROW
	DECLARE
	BEGIN
        IF INSERTING THEN
		INSERT INTO pracownicy_archiwum
	   (  EVENT_DATE	      
		, EVENT_TYPE          
		, USER_NAME           
		, OLD_ID 			  
		, OLD_IMIE 			  
		, OLD_NAZWISKO 		  
		, OLD_PESEL 		  
		, OLD_PLEC 			  
		, OLD_DATA_URODZENIA  
		, OLD_PENSJA 		  
		, OLD_KOD_DEPARTAMENTU
		, NEW_ID 			  
		, NEW_IMIE 			  
		, NEW_NAZWISKO 		  
		, NEW_PESEL 		  
		, NEW_PLEC 			  
		, NEW_DATA_URODZENIA  
		, NEW_PENSJA 		  
		, NEW_KOD_DEPARTAMENTU
	)
		VALUES
		(
		  SYSDATE
		, 'INSERT'
		, USER
		, NULL
		, NULL
		, NULL
		, NULL
		, NULL
		, NULL
		, NULL
		, NULL
		, :NEW.id
		, :NEW.IMIE 			  
		, :NEW.NAZWISKO 		  
		, :NEW.PESEL 		  
		, :NEW.PLEC 			  
		, :NEW.DATA_URODZENIA  
		, :NEW.PENSJA 		  
		, :NEW.KOD_DEPARTAMENTU
		);

        ELSIF UPDATING THEN

        INSERT INTO pracownicy_archiwum
	   (  EVENT_DATE	      
		, EVENT_TYPE          
		, USER_NAME           
		, OLD_ID 			  
		, OLD_IMIE 			  
		, OLD_NAZWISKO 		  
		, OLD_PESEL 		  
		, OLD_PLEC 			  
		, OLD_DATA_URODZENIA  
		, OLD_PENSJA 		  
		, OLD_KOD_DEPARTAMENTU
		, NEW_ID 			  
		, NEW_IMIE 			  
		, NEW_NAZWISKO 		  
		, NEW_PESEL 		  
		, NEW_PLEC 			  
		, NEW_DATA_URODZENIA  
		, NEW_PENSJA 		  
		, NEW_KOD_DEPARTAMENTU
	)
		VALUES
		(
		  SYSDATE
		, 'INSERT'
		, USER
        , :OLD.id
		, :OLD.IMIE 			  
		, :OLD.NAZWISKO 		  
		, :OLD.PESEL 		  
		, :OLD.PLEC 			  
		, :OLD.DATA_URODZENIA  
		, :OLD.PENSJA 		  
		, :OLD.KOD_DEPARTAMENTU
		, :NEW.id
		, :NEW.IMIE 			  
		, :NEW.NAZWISKO 		  
		, :NEW.PESEL 		  
		, :NEW.PLEC 			  
		, :NEW.DATA_URODZENIA  
		, :NEW.PENSJA 		  
		, :NEW.KOD_DEPARTAMENTU
		);
        -- deleting
        ELSE
        INSERT INTO pracownicy_archiwum
	   (  EVENT_DATE	      
		, EVENT_TYPE          
		, USER_NAME           
		, OLD_ID 			  
		, OLD_IMIE 			  
		, OLD_NAZWISKO 		  
		, OLD_PESEL 		  
		, OLD_PLEC 			  
		, OLD_DATA_URODZENIA  
		, OLD_PENSJA 		  
		, OLD_KOD_DEPARTAMENTU
		, NEW_ID 			  
		, NEW_IMIE 			  
		, NEW_NAZWISKO 		  
		, NEW_PESEL 		  
		, NEW_PLEC 			  
		, NEW_DATA_URODZENIA  
		, NEW_PENSJA 		  
		, NEW_KOD_DEPARTAMENTU
	)
		VALUES
		(
		  SYSDATE
		, 'INSERT'
		, USER
	    , :OLD.id
		, :OLD.IMIE 			  
		, :OLD.NAZWISKO 		  
		, :OLD.PESEL 		  
		, :OLD.PLEC 			  
		, :OLD.DATA_URODZENIA  
		, :OLD.PENSJA 		  
		, :OLD.KOD_DEPARTAMENTU
		, NULL
		, NULL			  
		, NULL		  
		, NULL 		  
		, NULL 			  
		, NULL  
		, NULL		  
		, NULL
		);
        END IF;
	END;
	
